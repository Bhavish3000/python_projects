   "metadata": {},
   "outputs": [],
   "source": [
    "number = int(input(\"Enter the number: \")) # Taking input as the number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for factor in range (2, number+1):\n",
    "    if number%factor == 0:\n",
    "        print(f\"{factor} is a factor {number}\")"
   ]
  }
